{
  "name": "pwoservers",
  "version": "0.0.1",
  "description": "PWO Servers",
  "main": "client/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:watch": "webpack --config webpack.config.js --watch",
    "test": "mocha --require ignore-styles --opts spec/mocha.opts ${FILE:-spec}",
    "test:clean": "mocha --opts spec/mocha.opts ${FILE:-spec}",
    "start": "node webpack_server.js",
    "cover": "nyc --require babel-core/register --reporter=text --all npm run test:clean"
  },
  "nyc": {
    "exclude": [
      "spec"
    ],
    "include": [
      "src"
    ]
  },
  "author": "Benjamin J. Anderson <andeb2804@gmail.com>",
  "license": "MIT",
  "repository": "git@github.com:vandise/ccb.git",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "gulp-util": "^3.0.4",
    "mocha": "^2.4.5",
    "nyc": "^6.4.4",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "testdouble": "^3.2.5",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "bunyan": "^1.5.1",
    "express": "^4.16.4",
    "json-loader": "^0.5.4",
    "socket.io": "^1.0.0",
    "webpack-node-externals": "^1.7.2",
    "yaml-loader": "^0.5.0"
  }
}
